{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/advancedsearch.html",
      "destination": "/advancedSearch.html"
    } ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
